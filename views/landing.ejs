  					];
  					//change photo on set interval of 2 seconds (with index variable)
  					var index = 0;
  					setInterval(function(){
  						$("body").css("background-image", "url(" + images[index] + ")");
  						index++;
  						index = index % 3;
  					}, 2000);
  				});
  				
  			</script>

